{
  "name": "nti-content",
  "version": "1.3.0",
  "author": "nextthought",
  "description": "ReactJS Editor and Renderer for content",
  "license": "UNLICENSED",
  "main": "./lib/index.js",
  "style": "./lib/index.css",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.3.0"
  },
  "scripts": {
    "test": "make test",
    "start": "make check; webpack-dev-server -d --config ./webpack.config.test.js  --host 0.0.0.0 --port 8000 --watch --progress --inline --info false --content-base ./test/app/ || true",
    "prepublish": "not-in-install && make || in-install",
    "bump": "npm version",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextThought/nti.web.component.content.git"
  },
  "keywords": [
    "react",
    "editor",
    "modeled",
    "modeled body",
    "content",
    "component"
  ],
  "dependencies": {
    "classnames": "^2.0.0",
    "draft-js": "^0.10.0",
    "draft-js-plugins-editor": "^2.0.0-beta1",
    "draft-js-utils": "^0.1.7",
    "fbjs": "^0.8.3",
    "filesize": "^3.3.0",
    "html-entities": "^1.2.0",
    "html-reactifier": "^1.0.1",
    "invariant": "^2.1.1",
    "linkify-it": "^2.0.0",
    "nti-commons": "^2.5.4",
    "nti-lib-content-processing": "^1.0.1",
    "nti-lib-dispatcher": "^1.0.0",
    "nti-lib-dom": "^1.3.3",
    "nti-lib-locale": "^1.0.7",
    "nti-lib-store": "^1.0.1",
    "nti-lib-whiteboardjs": "^1.3.0",
    "nti-util-ios-version": "^1.0.0",
    "nti-util-logger": "^1.0.0",
    "nti-web-commons": ">=0.0.1",
    "nti-web-video": "^1.3.0",
    "nti-web-whiteboard": "0.0.1",
    "object-hash": "^1.1.5",
    "prop-types": "^15.5.8",
    "punycode": "^1.4.1",
    "react-transition-group": "^1.1.0",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.1.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.1.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.1.0",
    "babel-preset-stage-1": "^6.1.0",
    "baggage-loader": "^0.3.0",
    "css-loader": "0.x.x",
    "enzyme": "^2.8.0",
    "eslint-config-nti-codestyle-js": "^1.1.0",
    "eslint-config-nti-codestyle-jsx": "^1.3.5",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "0.x.x",
    "in-publish": "^2.0.0",
    "jest": "^19.0.2",
    "jest-junit": "^1.4.0",
    "node-sass": "^4.5.0",
    "normalize.css": "^5.0.0",
    "nti-style-common": "^1.0.0",
    "postcss-loader": "^1.3.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "resolve-url-loader": "^2.0.0",
    "sass-loader": "^6.0.0",
    "style-loader": "0.x.x",
    "url-loader": "0.x.x",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.4.0"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.spec.js"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "testMatch": [
      "**/__test__/**/*.(spec).js?(x)"
    ]
  },
  "jest-junit": {
    "output": "reports/test-results/index.xml"
  }
}
